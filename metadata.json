{
  "name": "voxpupuli-jolokia",
  "version": "0.1.0",
  "author": "voxpupuli",
  "summary": "Install jolokia agents, and configure",
  "license": "Apache-2.0",
  "source": "https://github.com/voxpupuli/puppet-jolokia",
  "project_page": "https://github.com/voxpupuli/puppet-jolokia",
  "issues_url": "https://github.com/voxpupuli/puppet-jolokia/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
